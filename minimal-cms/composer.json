{
	"name": "jvdanker/minimal-cms",
	"description": "Example of minimal SilverStripe CMS",
	"require": {
		"silverstripe/cms": "3.5.3",
		"silverstripe-themes/simple": "3.1.*"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7@stable"
	},
	"extra": {
		"branch-alias": {
			"3.x-dev": "3.5.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}
